{
  "mosaics": [
    {
      "name": "Most recent",
      "cql": [
        {
          "op": "anyinteracts",
          "args": [
            { "property": "datetime" },
            { "interval": ["1998-01-01", "2012-01-01"] }
          ]
        }
      ]
    }
  ],
  "renderOptions": [
    {
      "name": "Default",
